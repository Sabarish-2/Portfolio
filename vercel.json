 {
     "builds": [
         {
             "src": "api/app.py",
             "use": "@vercel/python"
         },
         {
             "src": "public/*",
             "use": "@vercel/static"
         }
     ],
     "routes": [
         {
             "src": "/(.*)",
             "dest": "/public/$1"
         },
         {
             "src": "/api/(.*)",
             "dest": "api/app.py"
         }
     ]
 }
